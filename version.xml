   <service type="beast.base.core.BEASTInterface">
        <provider classname="morphmodels.app.beauti.BeautiMorphModelAlignmentProvider"/>
        <provider classname="morphmodels.evolution.alignment.AscertainedForParsimonyUninformativeAlignment"/>
        <provider classname="morphmodels.evolution.alignment.AscertainedForParsimonyUninformativeFilteredAlignment"/>
        <provider classname="morphmodels.evolution.substitutionmodel.LewisMK"/>
        <provider classname="morphmodels.evolution.substitutionmodel.NestedOrdinal"/>
        <provider classname="morphmodels.evolution.substitutionmodel.NStatesNoRatesSubstitutionModel"/>
